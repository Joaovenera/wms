{
  "name": "warehouse-frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:https": "vite --https",
    "build": "vite build",
    "start": "NODE_ENV=production node dist/index.js",
    "build:with-tsc": "tsc -p tsconfig.build.json && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "check": "tsc",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:performance": "vitest run src/test/performance --reporter=verbose",
    "performance:baseline": "node scripts/performance-validation.js baseline",
    "performance:current": "node scripts/performance-validation.js current",
    "performance:compare": "node scripts/performance-validation.js compare",
    "performance:full": "node scripts/performance-validation.js full",
    "lighthouse:audit": "node scripts/lighthouse-audit.js audit",
    "lighthouse:report": "node scripts/lighthouse-audit.js report",
    "bundle:analyze": "node scripts/bundle-analyzer.js full",
    "bundle:size": "node scripts/bundle-analyzer.js bundle",
    "bundle:deps": "node scripts/bundle-analyzer.js deps",
    "bundle:validate": "node scripts/bundle-strategy-validator.js",
    "build:enhanced": "node scripts/build-with-validation.js",
    "build:production": "npm run build:enhanced"
  },
