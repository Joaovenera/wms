{
  "name": "warehouse-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --env-file=.env  src/index.ts",
    "build": "tsc && esbuild src/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:mobile": "playwright test mobile-workflows.spec.ts",
    "test:e2e:performance": "playwright test --project=performance",
    "test:e2e:accessibility": "playwright test --project=accessibility",
    "test:e2e:visual": "playwright test --project=visual",
    "test:e2e:auth": "playwright test auth-flows.spec.ts",
    "test:e2e:warehouse": "playwright test warehouse-operations.spec.ts",
    "test:e2e:report": "playwright show-report",
    "test:integration": "vitest run --config vitest.unified.config.ts --reporter=verbose",
    "test:unit": "vitest run --config vitest.unified.config.ts --reporter=verbose",
    "test:performance": "tsx src/test/performance/cache-test-runner.ts",
    "test:performance:parallel": "tsx src/test/performance/cache-test-runner.ts --parallel",
    "test:performance:critical": "tsx src/test/performance/cache-test-runner.ts --skip-non-critical",
    "test:performance:ci": "tsx src/test/performance/cache-test-runner.ts --parallel --skip-non-critical --quiet",
    "test:cache:basic": "vitest run src/test/performance/cache-performance.spec.ts",
    "test:cache:monitoring": "vitest run src/test/performance/cache-monitoring.spec.ts",
    "test:cache:load": "vitest run src/test/performance/cache-load-testing.spec.ts",
    "test:cache:postgres": "vitest run src/test/performance/postgres-cache-integration.spec.ts",
    "test:cache:regression": "vitest run src/test/performance/cache-regression-testing.spec.ts",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "migrate:photo-thumbnails": "tsx --env-file=.env src/scripts/migrate-photo-thumbnails.ts",
    "fix:foreign-keys": "tsx --env-file=.env src/scripts/fix-foreign-keys.ts",
    "test:db": "NODE_ENV=test tsx --env-file=.env.test tests/helpers/create-test-db.ts",
    "test:setup": "npm run test:db && npm run db:migrate",
    "test:ci": "vitest run --coverage"
  },
  "dependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/ioredis": "^4.28.10",
    "@types/multer": "^2.0.0",
    "@types/pg": "^8.15.4",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.0",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.5",
    "dompurify": "^3.2.6",
    "dotenv": "^17.2.0",
    "drizzle-orm": "^0.39.1",
    "drizzle-zod": "^0.7.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "jsdom": "^26.1.0",
    "memoizee": "^0.4.17",
    "memorystore": "^1.6.7",
    "multer": "^2.0.2",
    "node-cache": "^5.1.2",
    "openid-client": "^6.6.1",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "rate-limit-redis": "^4.2.1",
    "redis": "^4.7.1",
    "sharp": "^0.34.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "ws": "^8.18.3",
    "zod": "^3.24.2",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.0",
    "@types/compression": "^1.8.1",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/cors": "^2.8.19",
    "@types/dompurify": "^3.0.5",
    "@types/express": "4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/express-session": "^1.18.0",
    "@types/jsdom": "^21.1.7",
    "@types/memoizee": "^0.4.12",
    "@types/node": "^20.16.11",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/sharp": "^0.31.1",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "allure-playwright": "^2.15.1",
    "axe-core": "^4.8.3",
    "axe-playwright": "^1.2.3",
    "drizzle-kit": "^0.30.4",
    "esbuild": "^0.25.0",
    "supertest": "^6.3.4",
    "tsx": "^4.19.1",
    "typescript": "5.6.3",
    "vitest": "^1.2.0",
    "wait-on": "^7.2.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8"
  }
}
